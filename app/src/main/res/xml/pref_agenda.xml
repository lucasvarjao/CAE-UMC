<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- NOTE: Hide buttons to simplify the UI. Users can touch outside the dialog to
         dismiss it. -->
    <!-- NOTE: ListPreference's summary should be set to its value by the activity code. -->
    <ListPreference
        android:defaultValue="6"
        android:entries="@array/pref_alcance_agenda_titles"
        android:entryValues="@array/pref_alcance_agenda_values"
        android:key="alcance_agenda"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="@string/pref_title_alcance_agenda" />

    <SwitchPreference
        android:defaultValue="false"
        android:key="agenda_switch"

        android:title="@string/pref_title_agenda_switch" />
    <!--android:summary="@string/pref_description_agenda_switch"-->
    <!-- This preference simply launches an intent when selected. Use this UI sparingly, per
         design guidelines. -->
    <Preference android:title="@string/pref_title_trocar_usuario"
        android:key="alterar_usuario"
        android:summary=""
        android:enabled="false"/>


</PreferenceScreen>
